{
  "name": "conwy",
  "version": "0.1.0",
  "description": "Homepage of Jonathan Conway",
  "author": {
    "email": "jon@conwy.co",
    "name": "Jonathan Conway",
    "url": "https://conwy.co"
  },
  "homepage": "https://conwy.co",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:watch": "concurrently \"npm run build:tailwind:watch\" \"npm run dev\"",
    "build": "next build",
    "build-gh-pages": "npm run build:tailwind && npm run build:all && touch out/.nojekyll",
    "build:all": "next build && npm run build:custom:all",
    "build:watch": "nodemon \"npm run build\" ./src --ext ts,tsx,mdx,css --ignore *.test.*",
    "start": "npx serve@latest out -p 4000",
    "start:watch": "concurrently \"npm run start\" \"npm run build:watch\"",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "npx surge ./out conwy.surge.sh",
    "build:tailwind": "npx tsx ./src/builders/exec.ts tailwind",
    "build:tailwind:watch": "nodemon --ext styles.ts,css --exec \"npm run build:tailwind\" --ignore ./src/tailwind-js-classes.json",
    "build:custom": "npx tsx ./src/builders/exec.ts",
    "build:custom:all": "npx tsx ./src/builders/exec.ts --all",
    "rebuild": "rm -rf .next out && npm run build",
    "rebuild-and-deploy": "npm run rebuild && npm run deploy",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@jonathanconway/tailwindjs": "^0.0.22",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "@mui/icons-material": "^6.2.0",
    "@next/mdx": "^14.1.0",
    "@next/third-parties": "^15.0.3",
    "@tailwindcss/forms": "^0.5.9",
    "@types/mdx": "^2.0.11",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vanilla-extract/css": "^1.16.1",
    "@xenova/transformers": "^2.17.0",
    "eslint": "8.43.0",
    "eslint-config-next": "^14.2.6",
    "hygen": "^6.2.11",
    "install": "^0.13.0",
    "langchain": "^0.2.17",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "marked": "^14.1.0",
    "next": "^14.2.6",
    "npm": "^10.5.0",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.35",
    "prismjs": "^1.29.0",
    "puppeteer": "^19.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.1",
    "react-tooltip": "^5.28.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.7.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/icons": "^1.2.10",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.0",
    "@types/luxon": "^3.4.2",
    "@types/pluralize": "^0.0.33",
    "@types/prismjs": "^1.26.3",
    "@vanilla-extract/next-plugin": "^2.4.7",
    "@vanilla-extract/webpack-plugin": "^2.3.16",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
