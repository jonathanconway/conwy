    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vanilla-extract/next-plugin": "^2.4.14",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vanilla-extract/webpack-plugin": "^2.3.16",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
